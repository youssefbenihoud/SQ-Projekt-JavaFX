<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.EventController">
   <children>
      <Label layoutX="149.0" layoutY="94.0" text="Date" />
      <DatePicker fx:id="datePicker" layoutX="225.0" layoutY="90.0" />
      <Label layoutX="146.0" layoutY="43.0" text="Name" />
      <TextField fx:id="name" layoutX="225.0" layoutY="39.0" prefHeight="26.0" prefWidth="174.0" />
      <Spinner fx:id="total" layoutX="225.0" layoutY="136.0" onKeyTyped="#verifyNumber" prefHeight="26.0" prefWidth="174.0" />
      <Label layoutX="140.0" layoutY="140.0" text="Number" />
      <Button fx:id="createButton" layoutX="146.0" layoutY="187.0" mnemonicParsing="false" onAction="#createEvent" prefHeight="26.0" prefWidth="267.0" text="Create" />
      <Label fx:id="errorLog" layoutX="260.0" layoutY="61.0" />
   </children>
</AnchorPane>
